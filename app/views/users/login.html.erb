<br/>  <br/><h1 align="center">Login</h1>

<center>
<%= form_tag({:controller  => :users, :action => :authenticate}, :method => "get") do %>
        <%= label_tag(:username, "Username") %>
        <%= text_field_tag(:username) %>       <br/>  <br/>
        <%= label_tag(:password, "Password") %>
        <%= password_field_tag(:password) %>       <br/> <br/>
        <%= submit_tag("Log in") %>
<% end %>
</center>
